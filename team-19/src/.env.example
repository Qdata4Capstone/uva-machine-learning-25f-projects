# Canvas LMS OAuth Configuration
# Get these by creating a Developer Key in your Canvas instance:
# Settings → Developer Keys → + Developer Key
CANVAS_CLIENT_ID=your_canvas_client_id_here
CANVAS_CLIENT_SECRET=your_canvas_client_secret_here
CANVAS_BASE_URL=https://canvas.instructure.com

# Notion OAuth Configuration
# Get these by creating an integration at: https://www.notion.so/my-integrations
NOTION_CLIENT_ID=your_notion_client_id_here
NOTION_CLIENT_SECRET=your_notion_client_secret_here

# LangSmith Observability
# Sign up at https://smith.langchain.com/
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT="https://api.smith.langchain.com"
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=